{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "lib-leocare-components": {
      "projectType": "library",
      "root": "projects/lib-leocare-components",
      "sourceRoot": "projects/lib-leocare-components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/lib-leocare-components/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/lib-leocare-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/lib-leocare-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/lib-leocare-components/src/test.ts",
            "tsConfig": "projects/lib-leocare-components/tsconfig.spec.json",
            "karmaConfig": "projects/lib-leocare-components/karma.conf.js"
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "styles": [
              "projects/lib-leocare-components/src/styles.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/themes/lara-light-purple/theme.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "configDir": "projects/lib-leocare-components/.storybook",
            "browserTarget": "lib-leocare-components:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/lib-leocare-components"
            ],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "styles": ["projects/lib-leocare-components/src/styles.css"],
            "configDir": "projects/lib-leocare-components/.storybook",
            "browserTarget": "lib-leocare-components:build",
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/lib-leocare-components"
            ],
            "outputDir": "storybook-static"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
